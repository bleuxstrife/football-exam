<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dicoding.exam.footballexam">

    <uses-permission android:name="android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme.NoActionBar">
        <activity
            android:name="com.dicoding.exam.footballexam.view.activity.MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.dicoding.exam.footballexam.view.activity.MatchDetailActivity" />
        <activity android:name="com.dicoding.exam.footballexam.view.activity.TeamDetailActivity"/>
        <activity android:name="com.dicoding.exam.footballexam.view.activity.PlayerDetailActivity"/>
        <activity android:name="com.dicoding.exam.footballexam.view.activity.SearchAcitivity"/>
    </application>

</manifest>